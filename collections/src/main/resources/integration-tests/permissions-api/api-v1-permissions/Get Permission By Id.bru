meta {
  name: Get Permission By Id
  type: http
  seq: 1
}

get {
  url: {{permissions-api}}/api/v1/permissions/:id
  body: none
  auth: inherit
}

params:path {
  id: {{permissionId}}
}

assert {
  res.status: eq 200
  res.body.name: eq {{permissionName}}
  res.body.type: eq {{permissionType}}
  res.body.action: eq {{permissionAction}}
}
