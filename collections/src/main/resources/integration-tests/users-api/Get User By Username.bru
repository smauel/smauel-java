meta {
  name: Get User By Username
  type: http
  seq: 3
}

get {
  url: {{users-api}}/api/v1/users/username/:username
  body: none
  auth: inherit
}

params:path {
  username: {{username}}
}

assert {
  res.body.username: eq {{username}}
  res.body.fullName: eq {{fullName}}
  res.body.email: eq {{email}}
  res.status: eq 200
}
