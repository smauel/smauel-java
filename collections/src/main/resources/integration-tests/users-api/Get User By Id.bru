meta {
  name: Get User By Id
  type: http
  seq: 2
}

get {
  url: {{users-api}}/api/v1/users/:id
  body: none
  auth: inherit
}

params:path {
  id: {{userId}}
}

assert {
  res.body.username: eq {{username}}
  res.body.fullName: eq {{fullName}}
  res.body.email: eq {{email}}
  res.status: eq 200
}
